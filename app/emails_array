const csv = require('csv-array');

function emails_batch(req) { 
	
	var emails = [];

	csv.parseCSV(req.files.list.path, function(data){
	var emails_batch = data;
	
		for (i=0; i < emails_batch.length; i++) {
			emails.push(emails_batch[i]);
			console.log(emails_batch[i]);
			
			console.log(req.body.subject);
		}
	}
	, false)
	
	return emails;
	
	}

module.exports = emails_batch;